{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "DWELLIR_KEY",
    "ANALYTICS_DASHBOARD_AUTH_TOKEN",
    "ANALYTICS_DASHBOARD_BASE_URL",
    "COINBASE_SEED"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "watch": {
      "cache": false,
      "persistent": true
    },
    "//#format-and-lint": {},
    "//#format-and-lint:fix": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "prebuild": {
      "cache": false
    },
    "e2e": {
      "cache": false
    },
    "e2e:ui": {
      "cache": false
    },
    "e2e:debug": {
      "cache": false
    },
    "e2e:codegen": {
      "cache": false
    }
  }
}
